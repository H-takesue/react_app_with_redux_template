{
  "name": "template",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@stripe/react-stripe-js": "^1.6.0",
    "@stripe/stripe-js": "^1.21.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/firebase": "^3.2.1",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-persist": "^4.3.1",
    "@types/underscore": "^1.11.3",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "env-cmd": "^10.1.0",
    "history": "^5.2.0",
    "moment": "^2.29.1",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loader-spinner": "^4.0.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-saga-test-plan": "^4.0.4",
    "typescript": "^4.1.2",
    "underscore": "^1.13.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true REACT_APP_BUILD=local react-scripts start",
    "build": "react-scripts build",
    "test": "SKIP_PREFLIGHT_CHECK=true rREACT_APP_BUILD=local react-scripts test",
    "eject": "react-scripts eject",
    "test:staging": "env-cmd -f ./.env.staging npm run test -- --watchAll=false",
    "test:prod": "env-cmd -f ./.env npm run test -- --watchAll=false",
    "build:staging": "env-cmd -f ./.env.staging npm run build && npm run build-storybook",
    "build:prod": "env-cmd -f ./.env npm run build",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s ./public -o build/storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.5",
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/node-logger": "^6.3.5",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.5",
    "@types/react-swipe": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "css-loader": "^3.4.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-app": "^6.2.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.2",
    "firebase-tools": "^9.2.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sass": "^1.48.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.21.0",
    "url-loader": "^4.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --max-warnings=0 --ext .tsx --ext .ts --fix",
    "*.{css,scss}": "stylelint --fix"
  }
}
